<html>
	<head>
		<meta charset="utf-8"/>
		<meta http-equiv="X-UA-Compatible" content="IE=EDGE,chrome=1" />
		<title>Test report<#= TestSuites.Name #></title>
		<link rel="stylesheet" type="text/css" href="report.css" />
	</head>
	<body>
		<div>
			<div>
				<h1>Test report - detail</h1>
			</div>
			<div>
				<a href="index.html">TOP</a>
			</div>
			<div>
			<# foreach (var testSuite in TestSuites.TestItems) { #>
				<table>
					<caption><#= testSuite.Name #></caption>
					<tbody>
						<tr>
							<th>test name</th>
							<th>State</th>
							<th>Result</th>
							<th>Judge</th>
							<th>Timestamp</th>
							<th>Time</th>
						</tr>
						<# foreach (var testCase in testSuite.TestCases) { #>
						<tr>
							<td><#= testCase.Name #></td>
							<td><#= testCase.Status #></td>
							<td><#= testCase.Result #></td>
							<td><#= testCase.Judge #></td>
							<td><#= testCase.Timestamp #></td>
							<td><#= testCase.Time #></td>
						</tr>
						<# } #>
					</tbody>
				</table>
			<# } #>
			</div>
		</div>
	</body>
</html>

